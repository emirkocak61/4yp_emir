<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <arg name="isSim" default="false"/>
    <arg name="base_dof" default="6" />
    <param name="base_dof" value="$(arg base_dof)" />
    <!-- Control Nodes-->
    <node name="unitree_ros_interface" pkg="anytree_control" type="UnitreeSim" output="screen" if="$(arg isSim)"/>
    <node name="unitree_ros_interface" pkg="anytree_control" type="UnitreeHw" output="screen" unless="$(arg isSim)" />
    <node name="anytree_base_control" pkg="anytree_control" type="anytree_base_control.py" output="screen" />

</launch>
